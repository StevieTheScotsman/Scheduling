@model List<Scheduling.Models.ProjectProfileType>
@{
    ViewBag.Title = "List Project Profile Types";
    Layout = "~/Views/Shared/_ResponsiveLayout.cshtml";
}
@{ if (Model.Count == 0)
   {
    <p>
        There are no results</p>
   }
}
<div class='container'>
    <div class='sixteen columns'>
        @{
            if (Model.Count > 0)
            {
                
            @Html.Partial("_TableHeaderPartial", ViewData["header"] = "Baseline Types")
             
                
            <table class='condensed zebra-striped'>
                <thead>
                    <tr>
                        <th>
                            ID
                        </th>
                        <th>
                            Description
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @{  foreach (Scheduling.Models.ProjectProfileType ppt in Model)
                        {
                           
                        <tr>
                            <td>@ppt.ID
                            </td>
                            <td>@ppt.Description
                            </td>
                        </tr>
      
         
                        }
     
                    }
                </tbody>
            </table> 
 
        

            }


        }
    </div>
</div>
