@model List<Scheduling.Models.ProjectNewstand>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_ResponsiveLayout.cshtml";
}
<h2>
    There are @Model.Count Current Projects With News Stand Dates

</h2>


@{
    
       <!--use zebra-striped class for row-styling'-->
    
    if (Model.Count > 0)
    {
        
     <table cellspacing='0' cellpadding='0' class='zebra-striped'>
        <thead>
            <tr style='background-color: #808080'>
                <th>
                    Project
                </th>
                <th>
                    N/S Date
                </th>
            </tr>
        </thead>
        <tbody>
            @{
                  

        foreach (Scheduling.Models.ProjectNewstand pn in Model)
        {
                <tr>
                    <td style='width: 25%'>@pn.ProjectName
                    </td>
                    <td style='width: 25%'>@pn.NewstandDate
                    </td>
                </tr>

        }
                   
            }
        </tbody>
    </table>

        
    }
    
}
